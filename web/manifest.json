{
  "manifest_version": 3,
  "version": "0.1",
  "name": "Pieces Pie",
  "description": "This extension is written with dart/flutter code!",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "PiecesFD.png",
      "32": "icons/PiecesFD.png",
      "48": "icons/fox.png",
      "128": "icons/blue.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://music.yandex.ru/;"
}
}
